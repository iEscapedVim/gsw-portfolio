---
import { getCollection } from 'astro:content';
import IconWidget from "@comps/widgets/iconWidgetCard2.astro";
const procurementContent = await getCollection('iconwidgets', ({ id }) => id.startsWith('procurement/'));
procurementContent.reverse();
---
<div class="grid grid-cols-1 md:grid-cols-2 justify-center items-start gap-10 py-8 my-12">
    {procurementContent.map((p) => <IconWidget post={p}/>)}
</div>